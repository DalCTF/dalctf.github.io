---
import Banner from "../components/Banner.astro";
import Hero from "../components/Hero.astro";
import Spacer from "../components/Spacer.astro";
import Tag from "../components/Tag.astro";
import Layout from "../layouts/Layout.astro";
import { hexFor } from "../util/colors";
---

<Layout title="Weekly Problem | Status 418" description="">
	<!-- Hero -->
	<Hero upper="Weekly Problem" title="Forky">
		<Tag
			type="border"
			color={hexFor("yellow")}
			text="Reverse Engineering"
		/>
	</Hero>

	<!-- Banner -->
	<Banner
		icon="archive"
		text="Download Problem"
		url="https://play.picoctf.org/practice/challenge/24"
	/>
	<Spacer />

	<!-- Text -->
	<section>
		<p>
			In this program, identify the last integer value that is passed as
			parameter to the function doNothing().
		</p>
	</section>
</Layout>
